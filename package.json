{
  "name": "insights-in-motion",
  "version": "1.0.0",
  "main": "index.tsx",
  "author": "Marcelo Cardoso",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js",
    "client": "webpack-dev-server --progress",
    "client-wait": "wait-on tcp:8080 && yarn run client",
    "types": "gql-gen --config codegen.yml",
    "server": "nodemon -r dotenv/config",
    "start": "cross-env NODE_ENV=development concurrently \"yarn run server\" \"yarn run client-wait\"",
    "start-with-engine": "cross-env NODE_ENV=withEngine concurrently \"yarn run server\" \"yarn run client-wait\"",
    "test-wait": "wait-on tcp:8080 && jest",
    "test-alone": "cross-env NODE_ENV=development concurrently --kill-others --success first \"yarn run server\" \"yarn run test-wait\"",
    "test": "jest",
    "lint": "tslint --init tslint.json 'src/**/*.ts'",
    "launch": "cross-env NODE_ENV=production ts-node --project ./src/server/tsconfig.json -r tsconfig-paths/register ./src/server/index.ts"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^8.1.1",
